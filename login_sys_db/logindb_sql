create table register
(
    id              integer      null,
    register_status varchar(255) null,
    user_login_id   integer      null
);

create table users
(
    id       integer      null,
    email    varchar(255) null,
    pwd      varchar(255) null,
    username varchar(255) null
);

create table teams
(
    id             integer      null,
    team_name      varchar(255) null,
    member_1       varchar(255) null,
    member_2       varchar(255) null,
    member_3       varchar(255) null,
    member_4       varchar(255) null,
    team_leader_id integer      null,
    points         bigint       null
);

